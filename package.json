{
  "name": "poc-shell-temp-in-webpack",
  "displayName": "poc_shell_temp_in_webpack",
  "description": "poc_shell_temp_in_webpack",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "poc-shell-temp-in-webpack.helloWorld",
        "title": "PoC Shell Temp: Hello World"
      },
      {
        "command": "poc-shell-temp-in-webpack.loadText",
        "title": "PoC Shell Temp: Load text"
      },
      {
        "command": "poc-shell-temp-in-webpack.loadShell",
        "title": "PoC Shell Temp: Load from shell script"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "webpack -c webpack.config.test.js",
    "watch-tests": "yarn run compile-tests -w",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.14.1",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "mocha-webpack": "^1.1.0",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
